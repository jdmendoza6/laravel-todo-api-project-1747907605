{
  "family": "laravel-todo-api-service",
  "executionRoleArn": "arn:aws:iam::ACCOUNT_ID:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "laravel-todo-api-container",
      "image": "ACCOUNT_ID.dkr.ecr.ap-southeast-1.amazonaws.com/laravel-todo-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "DB_HOST_PLACEHOLDER"
        },
        {
          "name": "DB_DATABASE",
          "value": "DB_DATABASE_PLACEHOLDER"
        },
        {
          "name": "DB_USERNAME",
          "value": "DB_USERNAME_PLACEHOLDER"
        },
        {
          "name": "DB_PASSWORD",
          "value": "DB_PASSWORD_PLACEHOLDER"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/laravel-todo-api-service",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
